{
  "name": "brandnudge-api",
  "version": "1.0.0",
  "description": "Koa.js backend for Brand Nudge",
  "main": "dist/src/server.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/server.js",
    "dev": "node scripts/dev.mjs tsx watch src/server.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky || true",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.3.2",
    "dotenv": "^17.3.1",
    "inflection": "^1.13.4",
    "ioredis": "^5.9.3",
    "koa": "^3.1.2",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^13.1.0",
    "pg": "^8.18.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7"
  },
  "pnpm": {
    "packageExtensions": {
      "sequelize": {
        "dependencies": {
          "inflection": "^1.13.4"
        }
      }
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa__router": "^12.0.4",
    "@types/node": "^22.10.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^10.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "supertest": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
